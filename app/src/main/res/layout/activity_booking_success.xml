<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".activity.ActivityBookingSuccess">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/bookingsuccess_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/teal_700"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Đăng ký thành công"
        app:titleTextColor="@color/white" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Mã tuyến:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/bookingsuccess_id"
        app:layout_constraintEnd_toStartOf="@+id/bookingsuccess_id"
        app:layout_constraintHorizontal_bias="0.848"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/bookingsuccess_id"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/edittext_bg"
        android:clickable="false"
        android:cursorVisible="false"
        android:elevation="3dp"
        android:focusable="false"
        android:hint="ID"
        android:padding="7dp"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookingsuccess_toolbar" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Điểm đi:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/bookingsuccess_to"
        app:layout_constraintEnd_toStartOf="@+id/bookingsuccess_to"
        app:layout_constraintHorizontal_bias="0.848"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/bookingsuccess_from"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/edittext_bg"
        android:clickable="false"
        android:cursorVisible="false"
        android:elevation="3dp"
        android:focusable="false"
        android:hint="Điểm khởi hành"
        android:padding="7dp"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookingsuccess_to" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Điểm đến:"
        android:textColor="@color/black"
        android:textSize="18sp"

        app:layout_constraintBaseline_toBaselineOf="@+id/bookingsuccess_from"
        app:layout_constraintEnd_toStartOf="@+id/bookingsuccess_from"
        app:layout_constraintHorizontal_bias="0.848"
        app:layout_constraintStart_toStartOf="parent" />


    <EditText
        android:id="@+id/bookingsuccess_to"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/edittext_bg"
        android:clickable="false"
        android:cursorVisible="false"
        android:elevation="3dp"
        android:focusable="false"
        android:hint="Điểm đến"
        android:padding="7dp"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookingsuccess_id" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Giờ đi:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/bookingsuccess_time"
        app:layout_constraintEnd_toStartOf="@+id/bookingsuccess_time"
        app:layout_constraintHorizontal_bias="0.848"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Ngày đi:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/bookingsuccess_date"
        app:layout_constraintEnd_toStartOf="@+id/bookingsuccess_date"
        app:layout_constraintHorizontal_bias="0.848"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/bookingsuccess_date"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/edittext_bg"

        android:cursorVisible="false"
        android:elevation="3dp"

        android:hint="Ngày khởi hành"
        android:padding="7dp"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookingsuccess_from" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Số ghế:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/bookingsuccess_seats"
        app:layout_constraintEnd_toStartOf="@+id/bookingsuccess_seats"
        app:layout_constraintHorizontal_bias="0.848"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/bookingsuccess_time"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/edittext_bg"

        android:cursorVisible="false"
        android:elevation="3dp"
        android:hint="Giờ khởi hành"
        android:padding="7dp"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookingsuccess_date" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Giá vé:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/bookingsuccess_price"
        app:layout_constraintEnd_toStartOf="@+id/bookingsuccess_price"
        app:layout_constraintHorizontal_bias="0.848"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/bookingsuccess_seats"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@drawable/edittext_bg"
        android:clickable="false"
        android:cursorVisible="false"
        android:elevation="3dp"
        android:focusable="false"
        android:hint="Số ghế"
        android:padding="7dp"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookingsuccess_time" />

    <EditText
        android:id="@+id/bookingsuccess_seated"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/edittext_bg"
        android:clickable="false"
        android:cursorVisible="false"
        android:elevation="3dp"
        android:focusable="false"
        android:hint="Số chổ còn trống"
        android:padding="7dp"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookingsuccess_price" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Tổng tiền:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/bookingsuccess_numberseat"
        app:layout_constraintEnd_toStartOf="@+id/bookingsuccess_numberseat"
        app:layout_constraintHorizontal_bias="0.855"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/bookingsuccess_numberseat"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:background="@drawable/edittext_bg"
        android:clickable="false"
        android:cursorVisible="false"
        android:elevation="3dp"
        android:focusable="false"
        android:hint="Tổng tiền"
        android:padding="7dp"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookingsuccess_seated" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Số ghế đặt:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/bookingsuccess_seated"
        app:layout_constraintEnd_toStartOf="@+id/bookingsuccess_seated"
        app:layout_constraintHorizontal_bias="0.848"
        app:layout_constraintStart_toStartOf="parent" />

 

    <EditText
        android:id="@+id/bookingsuccess_price"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/edittext_bg"
        android:clickable="false"
        android:cursorVisible="false"
        android:elevation="3dp"
        android:focusable="false"
        android:hint="Giá vé"
        android:padding="7dp"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookingsuccess_seats" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bookingview_admin"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_bg_detailed"
        android:text="Về trang chủ"
        android:layout_marginTop="20dp"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookingsuccess_numberseat" />

    <TextView
        android:layout_width="340dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:gravity="center_horizontal"
        android:text="Vui lòng chuẩn bị đủ số tiền và đến đúng lúc để có một chuyến đi thật suôn sẻ nhé"
        android:textAlignment="center"
        android:textColor="@color/teal_700"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bookingview_admin" />

</androidx.constraintlayout.widget.ConstraintLayout>